{
    "ver": "1.3.0",
    "authors": [
        "fliu","chenry"
    ],
    "contact": "https://kbase.us/contact-us/",
    "visible": true,
    "categories": ["active"],
	"widgets": {
        "input": null,
        "output": "no-display"
    },
    "parameters": [
        {
         "textsubdata_options" : {
            "show_src_obj" : true,
            "allow_custom" : false,
            "multiselection" : true,
            "subdata_selection" : {
               "selection_id" : "id",
               "parameter_id" : "query_model_ref",
               "path_to_subdata" : [
                  "modelreactions"
               ],
               "subdata_included" : [
                  "modelreactions/[*]/id",
                  "modelreactions/[*]/name"
               ],
               "selection_description" : [
                  "name"
               ]
            }
         },
         "text_options" : {
            "valid_ws_types" : []
         },
         "default_values" : [
            ""
         ],
         "advanced" : false,
         "id" : "reaction_set",
         "allow_multiple" : false,
         "optional" : false,
         "field_type" : "textsubdata"
        },{
            "id": "query_genome_ref",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "valid_ws_types": [ "KBaseGenomeAnnotations.Genome", "KBaseGenomes.Genome" ]
            }
        },{
            "id": "query_model_ref",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "valid_ws_types": [ "KBaseFBA.FBAModel" ]
            }
        },{
            "id": "feature_set_prefix",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options" : {
                "is_output_name" : true,
                "valid_ws_types": [ "KBaseCollections.FeatureSet"]
            }
        },
        {
            "id": "structural_similarity_floor",
            "optional": false,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "0.5" ],
            "field_type": "text",
            "text_options": {
                "validate_as": "float",
                "min_float" : 0.0,
                "max_integer" : 1.0
            }
        },
        {
            "id": "difference_similarity_floor",
            "optional": false,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "0.5" ],
            "field_type": "text",
            "text_options": {
                "validate_as": "float",
                "min_float" : 0.0,
                "max_integer" : 1.0
            }
        },
        {
            "id": "blast_score_floor",
            "optional": false,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "50.0" ],
            "field_type": "text",
            "text_options": {
                "validate_as": "float",
                "min_float" : 0.0,
                "max_integer" : 1000.0
            }
        },
        {
            "id": "number_of_hits_to_report",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "10" ],
            "field_type": "text",
            "text_options": {
                "validate_as": "int",
                "min_integer" : 0,
                "max_integer" : 50
            }
        }
    ],
    "behavior": {
        "service-mapping": {
            "url": "",
            "name": "kb_pickaxe",
            "method": "find_genes_for_novel_reactions",
            "input_mapping": [
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                },{
                    "narrative_system_variable": "workspace_id",
                    "target_property": "workspace_id"
                },{
                    "input_parameter": "reaction_set",
                    "target_property": "reaction_set"
                },{
                    "input_parameter": "structural_similarity_floor",
                    "target_property": "structural_similarity_floor"
                },{
                    "input_parameter": "difference_similarity_floor",
                    "target_property": "difference_similarity_floor"
                },{
                    "input_parameter": "blast_score_floor",
                    "target_property": "blast_score_floor"
                },{
                    "input_parameter": "query_genome_ref",
                    "target_property": "query_genome_ref",
                    "target_type_transform": "resolved-ref"
                },{
                    "input_parameter": "query_model_ref",
                    "target_property": "query_model_ref",
                    "target_type_transform": "resolved-ref"
                },{
                    "input_parameter": "feature_set_prefix",
                    "target_property": "feature_set_prefix"
                },{
                    "input_parameter": "number_of_hits_to_report",
                    "target_property": "number_of_hits_to_report"
                }
            ],
            "output_mapping": [
                {
                    "service_method_output_path": [0,"report_name"],
                    "target_property": "report_name"
                },
                {
                    "service_method_output_path": [0,"report_ref"],
                    "target_property": "report_ref"
                },
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                }
            ]
        }
    },
    "job_id_output_field": "docker"
}
